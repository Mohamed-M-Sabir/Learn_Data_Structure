#ifndef STACK_H_
#define STACK_H_
#include "std_types.h"
#define STACK_MAX_SIZE 5
typedef struct {

	uint32_t stack_arr[STACK_MAX_SIZE];
	uint32_t stack_ptr;

}stack_type;

void stack_init(stack_type *mystack);
#endif /* STACK_H_ */
