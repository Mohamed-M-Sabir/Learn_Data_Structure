#include "std_types.h"
#include "stack.h"
#include <stdio.h>


ret_status stack_init(stack_type *mystack)
{

	ret_status ret;
	if(NULL==mystack)
	{
		ret=NOK;
	}
	else
	{
		ret=OK;
		mystack->stack_ptr=-1;

		printf("new stack is created\n");
	}

	return ret;
}
